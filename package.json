{
  "name": "volantis-mydata",
  "version": "1.5.3",
  "description": "VOLANTIS MYDATA client app",
  "repository": "https://github.com/volantis-technology/volantis-mydata.git",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8.11.3",
    "yarn": "^1.12.3"
  },
  "scripts": {
    "analyze": "run-s build build:with-stats start:analyzer",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > dist/client/static/bundle-stats.json",
    "lint": "run-p lint:js lint:css",
    "lint:js": "esw --ext .jsx --ext .js --color",
    "lint:css": "stylelint ./src/**/*.js",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "start:analyzer": "webpack-bundle-analyzer dist/client/static/bundle-stats.json",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "npm run test --updateSnapshot",
    "package:build": "rm -rf dist && rollup -c",
    "package:build-watch": "rm -rf dist && rollup -c -w",
    "package:modify": "node scripts/modify-package.js",
    "package:revert": "node scripts/revert-package.js",
    "prerelease": "run-s lint package:build package:modify",
    "release": "npm publish; npm run package:revert",
    "precommit:msg": "echo 'Linter checks...' && exit 0"
  },
  "pre-commit": [
    "precommit:msg",
    "lint"
  ],
  "moduleRoots": [
    "app/client",
    "app/server",
    "app/shared",
    "app",
    "node_modules"
  ],
  "publishConfig": {
    "registry": "http://repo.volantis.io/repository/npm/"
  },
  "dependencies": {
    "react-dropzone": "^10.1.4",
    "react-pure-lifecycle": "^3.0.0",
    "tus-js-client": "^1.8.0-1"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "react-redux": ">=7.0.0",
    "styled-components": ">=4.2.0",
    "volantis-icon": ">=1.0.0",
    "volantis-ui": ">=1.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
