{
  "name": "volantis-mydata",
  "version": "1.1.0",
  "description": "VOLANTIS MYDATA client app",
  "repository": "https://github.com/volantis-technology/volantis-mydata.git",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "publishConfig": {
    "registry": "http://repo.volantis.io/repository/npm/"
  },
  "scripts": {
    "start": "concurrently -k \"npm run build:watch\" \"cd example-cra; npm run start\"",
    "start:next": "concurrently -k \"npm run build:watch\" \"cd example-next; npm run dev\"",
    "build": "npm run delete-dist && rollup -c",
    "build:watch": "rollup -c -w",
    "lint": "esw --ext .jsx --ext .js --color",
    "lint:css": "stylelint './src/**/*.js'",
    "lints": "npm run lint && npm run lint:css",
    "pack": "node scripts/modifyPackage.js",
    "pack:revert": "node scripts/revertPackage.js",
    "prerelease": "npm run lints && npm run build && npm run pack",
    "release": "npm publish; npm run pack:revert",
    "delete-dist": "rm -rf dist",
    "precommit-msg": "echo 'Linter checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lints"
  ],
  "license": "ISC",
  "dependencies": {
    "filesize": "^4.1.2",
    "moment": "^2.24.0",
    "polished": "^3.0.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.4.2",
    "react-dropzone": "^10.1.3",
    "react-pure-lifecycle": "^3.0.0",
    "tus-js-client": "1.5.2",
    "universal-cookie": "^3.1.0",
    "uuid": "^3.3.2",
    "volantis-icon": "^0.2.0",
    "volantis-ui": "^1.4.0"
  },
  "peerDependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3",
    "superagent": "^4.1.0",
    "universal-cookie": "^3.0.7",
    "volantis-icon": "^0.2.0",
    "volantis-ui": "^1.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "nodemon": "^1.18.10",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-url": "^2.2.0",
    "styled-components": "^4.2.0"
  },
  "files": [
    "dist"
  ]
}
