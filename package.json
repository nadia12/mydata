    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-manifest-plugin": "^2.0.4",
